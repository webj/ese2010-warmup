package models;

import javax.persistence.ManyToOne;

import play.db.jpa.Model;

public class Vote extends Model {

	public User author;
	public String result;

	@ManyToOne
	public Question text;

	public Vote(Question text, User author, String result) {

		this.text = text;
		this.author = author;
		this.result = result;

	}

}
